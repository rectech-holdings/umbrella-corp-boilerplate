{
  "name": "@acme-corp/db",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "tsc",
    "build:prisma": "prisma generate",
    "dev": "run-p -l dev:*",
    "dev:db": "node cli-scripts/start-dev-db.mjs",
    "dev:ts": "tsc -w --preserveWatchOutput",
    "dev:prisma": "prisma generate --watch",
    "create-migration": "prisma migrate dev --create-only",
    "apply-migrations": "prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "3.6.0",
    "dotenv": "10.0.0",
    "prisma": "3.6.0"
  },
  "devDependencies": {
    "@types/node": "17.0.0",
    "execa": "6.0.0",
    "npm-run-all": "4.1.5",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
