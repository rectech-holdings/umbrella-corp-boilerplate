{
  "name": "ac-db",
  "private": true,
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:prisma": "prisma generate",
    "build:ts": "tsc",
    "dev": "run-p -l dev:*",
    "dev:db": "node scripts/start-dev-db.mjs",
    "dev:ts": "tsc -w --preserveWatchOutput",
    "dev:prisma": "prisma generate --watch",
    "create-migration": "prisma migrate dev --create-only",
    "apply-migrations": "prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "4.0.0",
    "dotenv": "16.0.1",
    "prisma": "4.0.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "execa": "6.1.0",
    "npm-run-all": "4.1.5",
    "typescript": "4.7.4"
  }
}
